export PATH="/opt/local/bin:$PATH"

export PATH=".:bin:/usr/local/bin:/usr/local/sbin:$ZSH/bin:$HOME/bin:$PATH"

# z (aka build myself into myhome dir)
export PATH="$HOME/z/bin:$PATH"

